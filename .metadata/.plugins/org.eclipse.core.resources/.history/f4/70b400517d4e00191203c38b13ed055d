package com.class11;

import java.util.Scanner;

public class Main {
	/* Write a program that  takes a 2-D array and prints the sums of the rows as an integer array.
	 */
	public static void main(String[] args){
		int[][] a = {
				{5,2,3,7},
				{1,5,2,2},
				{1,2,3,4}
			};
			int sum=0;
			for (int b=0;b<a[2].length;b++) {
			sum=+a[2][b];}
			System.out.println(sum);
		}
	}