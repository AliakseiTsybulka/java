package com.class11;

import java.util.Scanner;

public class Main {
	/* For you to do:
If browser is ChRoME
It should print the 
Proceed with chrome Browser
If browser is FireFOX
It should print the 
Proceed with firefox Browser
If browser is Ie
It should print the 
Proceed with ie Browser
If any other browser 
It should print the  
Invalid Browser
	 */
	public static void main(String[] args)
	{
		int[][] a = {
			{1,1,2}, //sum = 4
			{3,1,2}, //sum = 6
			{3,5,3}, //sum = 11
			{0,1,2}  //sum = 3
		};
		int [] newArray=new int [a.length];
		int rowSums=0;
		for (int[] b:a) {
			for (int c:b) {
			rowSums+=b[c];}
			
			System.out.println(rowSums);
			}
		}
}