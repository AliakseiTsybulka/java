package com.class11;

import java.util.Scanner;

public class Main {
	/* Write a program that  takes a 2-D array and prints the sums of the rows as an integer array.
	 */
	public static void main(String[] args){
		int[][] a = {
			{1,1,2}, //sum = 4
			{3,1,2}, //sum = 6
			{3,5,3}, //sum = 11
			{0,1,2}  //sum = 3
		};
		int rowSums=0;
		for (int[] newArray : a) {
			for (int i=0;i<newArray.length;i++) {
			newArray[i]=rowSums+=newArray[i];}
			System.out.println(rowSums);
			rowSums=0;
			}
		}
}